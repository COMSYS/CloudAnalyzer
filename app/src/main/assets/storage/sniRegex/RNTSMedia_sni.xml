<DomainRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="RNTSMedia" Id="62">
  <Region ID="0">
    <!-- HeyZap -->
    <!-- Fyber (formerly SponsorPay) https://www.crunchbase.com/organization/sponsorpay -->
    <Regex Regex="(?:\S*\.)?(?:heyzap|haizap|fyber|sponsorpay)\.com" />
    <!-- Inneractive -->
    <Regex Regex="(?:\S*\.)?inner-active\.(?:com|mobi)" />
  </Region>
</Service>
</DomainRegexes>
