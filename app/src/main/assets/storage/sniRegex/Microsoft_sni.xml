<DomainRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="Microsoft" Id="51">
  <Region ID="0">
    <!-- HockeyApp -->
    <Regex Regex="(?:\S*\.)?hockeyapp\.(?:net|com)" />
    <!-- MicrosoftAzure -->
    <Regex Regex="(?:\S*\.)?(?:cloudapp|msecnd|azureedge|a-msedge|azurewebsites\.windows|azurewebsites|azure-mobile|azure-api|trafficmanager)\.net" />
    <Regex Regex="(?:\S*\.)?(?:aadrm|azurerms|azure)\.com" />
    <!-- MicrosoftOffice -->
    <Regex Regex="(?:\S*\.)?(?:aka|gfx|sfx|onestore|mcmexch)\.ms" />
    <Regex Regex="(?:\S*\.)?(?:bing|exchangelabs|getmicrosoftkey|lync|microsoft|msocdn|msocsp|office365|s-microsoft|omniroot|onedrive|onmicrosoft|outlook|s-msn|visualstudio|acompli|sharepoint|sharepointonline|sway|sway-cdn|sway-extensions|tific|yammer|yammerusercontent|skype|skypeassets|hotmail|hotmail-int|msn|msn-int|live-int|microsoftonline|microsoftstore|microsoftemail|microsoftonline-p|playmsn|msocdn|aspnetcdn|wunderlist)\.com" />
    <Regex Regex="(?:\S*\.)?(?:office|live|onenote)\.(?:com|net)" />
    <Regex Regex="(?:\S*\.)?(?:directtaps|osdinfra|msecnd|msedge|s-msedge|msft|cloudapp|skype|webtv)\.net" />
  </Region>
</Service>
</DomainRegexes>
