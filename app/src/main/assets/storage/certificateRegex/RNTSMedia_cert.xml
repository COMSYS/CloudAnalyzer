<CertificateRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="RNTSMedia" Id="62">
  <Region ID="0">
    <Regex Regex=".*CN=(?:\S*\.)?(?:heyzap|haizap|fyber|sponsorpay)\.com(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*O=&quot;Fyber GmbH&quot;(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*O=Fyber GmbH(?:,.*)?$" Type="Subject"/>
    <!-- Inneractive -->
    <Regex Regex=".*CN=(?:\S*\.)?inner-active\.(?:com|mobi)(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*" Type="AlternativeName"/>
  </Region>
</Service>
</CertificateRegexes>
