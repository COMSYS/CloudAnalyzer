<CertificateRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="Facebook" Id="31">
  <Region ID="0">
    <Regex Regex=".*O=&quot;Facebook, Inc\.&quot;(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*O=&quot;WhatsApp, Inc\.&quot;(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*CN=(?:\S*\.)?(?:instagram|whatsapp)\.com(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*" Type="AlternativeName"/>
  </Region>
</Service>
</CertificateRegexes>
