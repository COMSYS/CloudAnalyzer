<CertificateRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="Rackspace" Id="61">
  <Region ID="0">
    <Regex Regex=".*CN=(?:\S*\.)?(?:rackcdn|rackspacecloud)\.com(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*CN=(?:\S*\.)?rackspace\.(?:com|net|nl|com\.hk|com\.au|co\.uk)(?:,.*)?$" Type="Subject"/>
    <Regex Regex=".*" Type="AlternativeName"/>
  </Region>
</Service>
</CertificateRegexes>
