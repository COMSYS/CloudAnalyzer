<DomainRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="Twitter" Id="81">
  <Region ID="0">
    <!-- TweetDeck https://www.crunchbase.com/organization/tweetdeck -->
    <!-- Vine https://www.crunchbase.com/organization/vine -->
    <Regex Regex="(?:\S*\.)?(?:t|vine)\.co" />
    <Regex Regex="(?:\S*\.)?(?:twitter|ads-twitter|twttr|twimg|tweetdeck|vineapp)\.com" />
    <!-- MoPub -->
    <Regex Regex="(?:\S*\.)?mopub\.(?:com|link)" />
    <!-- AdGrok shutdown -->
  </Region>
</Service>
</DomainRegexes>
