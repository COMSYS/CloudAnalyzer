    <Regex Regex="(?:\S*\.)?fbstatic\.\S*akamai\S*" />
    <Regex Regex="(?:\S*\.)?fbcdn\.\S*akamai\S*" />
    <Regex Regex="(?:\S*\.)?fbexternal\.\S*akamai\S*" /> -->
  </Region>
</Service>
