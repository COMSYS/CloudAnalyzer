<DomainRegexes xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Service Name="Facebook" Id="31">
  <Region ID="0">
    <Regex Regex="(?:\S*\.)?(?:facebook|fb|messenger)\.(?:com|net|me)" />
    <Regex Regex="(?:\S*\.)?(?:fbcdn|fbsbx|tfbnw|whatsapp)\.(?:com|net)" />
    <!-- Instagram, Atlas and Parse-->
    <Regex Regex="(?:\S*\.)?(?:instagram|cdninstagram|atlassolutions|atdmt|atlassbx|parse)\.com" />
    <!-- Legacy from Muehmer (Akamai) - updated 02-21-2017 -->
